#include "stdafx.h";
#include "AnimalNode.h";

template<class T>
class CircularList{
public:
	void add(T data);
	void editCurrent();
	void remove();
	void displayCurrent();
	bool isEmpty();
	CircularList(){
		current = new AnimalNode<T>();
		current->next = current;
		current->previous = current;
		itemsInList = 0;
	}
private:
	AnimalNode<T> *current;
	int itemsInList;
};
